<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Member">
    <insert id="save" parameterType="member">
        insert into member_Gym(memberId,memberPassword,memberName,memberAge,memberPhone)
            values (#{memberId},#{memberPassword},#{memberName},#{memberAge},#{memberPhone})
    </insert>

    <select id="duplicate" parameterType="String" resultType="String">
        select memberId from member_Gym where memberId=#{memberId}
    </select>
</mapper>